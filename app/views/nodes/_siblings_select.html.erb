<div class="field is-horizontal">
  <%= f.label :parent_ancestors %>
  <% if f.object.ancestors.present? %>
    <% f.object.ancestors.reverse_each do |item| %>
      <%= render partial: 'nodes/siblings_options', locals: { f: f, node: item } %>
    <% end %>
  <% else %>
    <%= render partial: 'nodes/siblings_options', locals: { f: f, node: node } %>
  <% end %>
</div>
